{
    "name": "matijajanc/postman",
    "description": "Postman file generator",
    "keywords": ["laravel", "lumen", "postman"],
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Matijajanc\\Postman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Matijajanc\\Postman\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Matija Janc",
            "email": "matija.janc@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": ">=6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matijajanc\\Postman\\Providers\\PostmanServiceProvider"
            ]
        }
    }
}
